{
  "name": "canvan-sliders",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all clean build-typescript rollup docs",
    "docs": "typedoc --out docs --target es6 --theme minimal --mode file canvan-sliders",
    "build-typescript": "tsc --module umd",
    "rollup": "rollup -c rollup.config.js",
    "rollup-watch": "rollup -w -c rollup.config.js",
    "serve": "browser-sync start --server \"./examples\" --files \"./examples\"",
    "clean": "rimraf dist",
    "watch": "npm-run-all --parallel serve rollup-watch",
    "start": "npm-run-all clean watch",
    "test": "karma start"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "^3.5.9",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "browser-sync": "^2.26.7",
    "eslint": "^6.8.0",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.1.1",
    "karma-typescript": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.9",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-node": "^8.6.2",
    "typedoc": "^0.15.5",
    "typescript": "^3.7.3"
  }
}
